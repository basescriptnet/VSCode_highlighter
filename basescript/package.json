{
    "name": "basescript",
    "displayName": "BaseScript",
    "description": "Superset for JavaScript",
    "version": "0.1.1",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "bs",
            "aliases": ["base", "basescript", "bs", "bm"],
            "extensions": [".bs", ".bm"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "bs",
            "scopeName": "source.bs",
            "path": "./syntaxes/bs.tmLanguage.json"
        }]
    }
}
